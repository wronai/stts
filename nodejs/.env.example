# Copy to .env and adjust

# Cache dir (models, config)
STTS_CONFIG_DIR=

# Config file format preference: auto/json/yaml
STTS_CONFIG_FORMAT=auto

# Enable NLP2CMD integration (calls nlp2cmd CLI)
STTS_NLP2CMD_ENABLED=0
STTS_NLP2CMD_BIN=nlp2cmd
STTS_NLP2CMD_ARGS=-r
STTS_NLP2CMD_CONFIRM=1
# Optional: prewarm nlp2cmd in background (Python worker)
STTS_NLP2CMD_PARALLEL=
# Optional: python interpreter for the worker
STTS_NLP2CMD_PYTHON=

# Defaults
STTS_TIMEOUT=5
STTS_LANGUAGE=pl
STTS_STT_PROVIDER=
STTS_STT_MODEL=
STTS_STT_GPU_LAYERS=
STTS_STT_PROMPT=
STTS_TTS_PROVIDER=
STTS_TTS_VOICE=pl
STTS_AUTO_TTS=1
STTS_STREAM=
STTS_FAST_START=
STTS_GPU_ENABLED=

# Testing: when set to 1, STT is read from sidecar file.wav.txt
STTS_MOCK_STT=0
